//백준 15652 N과 M (4)
#include <iostream>
#include <algorithm>
#include <cstring>
#include <vector>
using namespace std;

int n,m;
int arr[8]={0,};
bool visited[8]={0,};

void dfs(int node, int cnt) { //브루트포스 
    
	if ( cnt-1 == m) {
		for(int i=1;i<=m;i++){
		    cout<<arr[i]<<" ";
		}
		cout << '\n';
		return;}
	
	for(int i=1;i<=n;i++){ //dfs가 불릴때마다 계속 새롭게 다시 시작하는거다. 
	    
	        visited[i]=1;
	        arr[cnt]=i;
	        dfs(i,cnt+1);
	        visited[i]=0;
	    
	}
	
}


int main() {
    cin>>n>>m;
    
    dfs(1,1);
}
