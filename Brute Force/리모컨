//백준 1107 리모컨
#include <string> //to_string(n) n->string으로
#include <vector>
#include <algorithm>
#include <iostream>
#include <cmath>
using namespace std;

vector<bool> broken(10,false);

bool click(int now){
    string s = to_string(now);
    for(int i = 0; i <s.length();i++){
        if(broken[s[i]-'0']) {return false;}}
        return true;}

int main(){
    int destination,n;    scanf("%d %d",&destination,&n);
    int b;
    for(int i = 0; i < n; i++){
        scanf("%d",&b);
        broken[b] = true;}
    string st = to_string(destination);
    int minimum = abs(destination-100); //최대로 잡아놓되, +로 계속 가는 걸로 잡아놈.
    for(int i = 0; i <= 1000000; i++){
        if(click(i)){
            int tmp = abs(destination-i)+to_string(i).length();
            minimum = min(minimum,tmp);        }    }
    
    printf("%d \n",minimum);
    return 0;
}
