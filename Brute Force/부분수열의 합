//백준 1182 부분수열의 합
#include <iostream>
#include <vector>
using namespace std;

int n,answer; int arr[30]; int cnt{0};

void dfs(int node, int sum) {
    if (node==n) { //손코딩 해보면 이 조건이 꼭 필요하다. 무한대로 dfs를 들어가기 때문에. 
        if(sum==answer) cnt++;
        return;}
    dfs(node+1, sum); //선택안하고 넘기는 경우 
    dfs(node+1, sum+arr[node]);} //선택하고 더하는 경우 

int main(void) {
    ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
    cin>>n>>answer;
    for(int i=0;i<n;i++) cin>>arr[i];
    dfs(0,0);
    if(answer==0) cnt--;
    cout<<cnt;}
