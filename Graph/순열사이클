// 백준 10451 순열 사이클 
#include<iostream>
#include<vector>
#include<queue>
using namespace std;
//LIST X DFS 로 해결하기: 4가지 다 가능하다. 
int t,n,no,ret{0};
bool visited[10001];
vector<int> li[10001];

void dfs(int node){
    visited[node]=1;
    for(auto i: li[node]){
        
        if(!visited[i]){
            dfs(i);
        }
        if(i==node) ret++;//1번째 CASE에서 답이 1로 오답이 나오는 이유 돌려보고 수정하기 
        
    }
    
}

int main(){
    cin.tie(0); cout.tie(0); ios_base::sync_with_stdio(false);
    cin>>t;
    while(t--){
    cin>>n;
    for(int i=1;i<=n;i++){
        cin>>no;
        li[i].push_back(no);
    }
    fill_n(visited,n,0);//방문안함으로 초기화 
    
    for(int i=1;i<=n;i++){
        if(!visited[i]){
            dfs(i);
        }
    }
    cout<<ret<<"\n";
}}
