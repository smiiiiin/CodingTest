//백준 11725 
#include <iostream>
#include <vector>
#include <queue>

using namespace std;

int par[100001];
bool visited[100001];
vector<int> adj[100001];

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    int n, a, b;
    queue<int> q;

    cin >> n;
    for(int i=1; i<n; i++) {
        cin >> a >> b;
        adj[a].emplace_back(b);
        adj[b].emplace_back(a);
    }

    visited[1] = true;
    q.push(1);
    while(!q.empty()) {
        int now = q.front();
        q.pop();
        for(int s: adj[now]) {
            if(!visited[s]) {
                par[s] = now;
                visited[s] = true;
                q.push(s);
            }
        }
    }
    for(int i=2; i<=n; i++) {
        cout << par[i] << '\n';
    }
    return 0;
}
