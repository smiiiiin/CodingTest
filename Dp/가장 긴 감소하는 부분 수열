//백준 11722

#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;
int main()
{
    int n;
    int a[1001];
    int dp[1001];

    cin >> n;
    fill(dp, dp+ n, 1);
    for (int i = 0; i < n; i++)
        cin >> a[i];
    
    
    for (int y = n-1;y>=0;y--)
    {
        for (int x = n-1; x>y; x--)
        {
            if (a[x] < a[y])
            {dp[y]=max(dp[y],dp[x]+1);}
        }
    }
    
    cout << *max_element(dp,dp+n);
}
