//백준 2011
#include <iostream>
#include <vector>
#include <string>
using namespace std;

#define MOD 1000000
int dp[5050] = {1};
int main(){
    string s;
    cin>>s;
    s = '#'+s;
    if(s[1] == '0') return cout<<0, 0;
    for(int i = 1 ; i < (int)s.size() ; i++){
        if(s[i]-'0'>0)dp[i] = (dp[i-1]+dp[i])%MOD;
        if(i>1 and s[i-1]-'0'>0 and stoi(string({s[i-1], s[i]})) <= 26)
            dp[i] = (dp[i-2]+dp[i])%MOD;
    }
    cout<<dp[(int)s.size()-1];
}
