//9465: sticker

#include <iostream>
#include <algorithm>
#define MAX 100001

using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);

    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        int dp[2][MAX];
        dp[0][0] = dp[1][0] = 0;
        for (int i = 1; i <= n; i++) {
            cin >> dp[0][i];
        }
        for (int i = 1; i <= n; i++) {
            cin >> dp[1][i];
        }
        
        for (int i = 2; i <= n; i++) {
            dp[0][i] = max(dp[1][i-1],dp[1][i-2])+dp[0][i];
            dp[1][i] = max(dp[0][i - 1], dp[0][i-2])+ dp[1][i];
        }
        int answer = max(dp[1][n], dp[0][n]);
        cout << answer << '\n';
    }
    


    return 0;
}

//백준 9465
//맞왜틀? 대각선 전에서 오는 경우, 전전대각선에서 오는 경우 (2)중에 max+ a[] 하면 되는거 아닌가? 

#include <iostream>
#include <algorithm>
using namespace std;
int t; int n;
long long dp[2][101]={0,};
long long a[2][101];


int main(void)
{   cin.tie(0); cout.tie(0); ios_base::sync_with_stdio(false);
    cin>>t;
    while(t--){
    cin>>n;
    for(int y=0;y<2;y++){
        for(int x=1;x<=n;x++){
            cin>>a[y][x];
        }}
    dp[0][1]= a[0][1]; dp[1][1]= a[1][1];
    dp[0][2]= a[0][2]+a[1][1]; dp[1][2]= a[1][2]+a[0][1];
    
    for(int x=3;x<=n;x++){
        for(int y=0;y<2;y++){
            dp[y][x]= a[y][x]+ max(dp[1-y][x-1], dp[1-y][x-2]);
        }
    }
        cout<<max(dp[0][n], dp[1][n])<<endl;
        
    }
    return 0;
}
