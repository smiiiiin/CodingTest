//백준 11053
//이것도 돼고 
#include <iostream>
#include <vector>
#include <bits/stdc++.h>

using namespace std;
int main()
{
    int n;
    int a[1001];
    int dp[1001];

    cin >> n;
    fill(dp, dp+ n, 1);
    for (int i = 0; i < n; i++)
        cin >> a[i];
    
    
    for (int i = 1; i < n; i++)
    {
        for (int j = 0; j<i; j++)
        {
            if (a[j] < a[i])
            {dp[i]=max(dp[i],dp[j]+1);}
            
            //위에 if문 아래 주석단 걸로 대체해도 된다 : 다음문제 11055풀려고 
            /*
            if(dp[i]<dp[j]+1){
                    dp[i]=dp[j]+1;
                }
                */
        }
    }
    
    cout << *max_element(dp,dp+n);
}


//백준 11053 왜 마지막에 +1을 해야할까? 
#include <iostream>
#include <algorithm>
#include <vector>

using namespace std;
int n;
int a[1001];
int dp[1001]={1, };

int main(void)
{
    cin>>n;
    for(int i=1;i<=n;i++){
        cin>>a[i];
    }
    for(int y=1;y<=n;y++){
        for(int x=1;x<y;x++){
            if(a[y]>a[x]){
            dp[y]=max(dp[y], dp[x]+1);
        }
        }}
    cout<<*max_element(dp+1, dp+n+1)+1; //왜 여기서 +1을 해야할까?
    return 0;
}
