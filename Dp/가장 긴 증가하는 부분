//백준 11053
#include <iostream>
#include <vector>

using namespace std;
int main()
{
    int n;
    int a[1001];
    int dp[1001];

    cin >> n;
    fill(dp, dp + n, 1);
    for (int i = 0; i < n; i++)
        cin >> a[i];
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j<i; j++)
        {
            if (a[j] < a[i])
                dp[i] = max(dp[i], dp[j] + 1);
        }
    }
    cout << *max_element(dp,dp+n);
}
