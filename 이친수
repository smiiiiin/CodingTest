//백준 2193

#include<iostream>
typedef long long ll;
using namespace std;
int n;
ll dp[3][105];

int main(){
    ll res{0};
    cin>>n;
    dp[0][0]=0; dp[1][0]=1;
    
    for(int x=0;x<n;x++){
        
            dp[1][x]=dp[0][x-1];
            dp[0][x]=(dp[0][x-1]+dp[1][x-1]);
        
    }
    
    for(int y=0;y<2;y++){
        res+= dp[y][n-1];
        
    }
    
    cout<<res<<"\n";
    return 0;
}
