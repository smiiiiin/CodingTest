// 1182 부분수열의 합
#include <iostream>
using namespace std;
int a[20]; int n, m; int ans = 0;

void dfs(int i, int sum) {
    if (i == n) {
        if (sum == m) ans += 1;
        return;}
    
    dfs(i+1, sum +a[i]);
    dfs(i+1, sum);}

int main() {
    cin >> n >> m;
    for (int i=0; i<n; i++) cin >> a[i];
    dfs(0, 0);
    if (m == 0) ans -= 1;
    cout << ans << '\n';
    return 0;}
