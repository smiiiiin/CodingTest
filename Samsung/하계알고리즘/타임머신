// 11657 타임머신
#include<iostream>
#include<algorithm>
#include<vector>
#include<queue>
using namespace std;
#define INF 987654321

int n, m; int A, B, C; int a, b;
vector<pair<int, int>> adj[501]; //입력
long long ans[501]; //출력
bool cycle_of_minus; //음의사이클


int main(){
	ios_base::sync_with_stdio(0); cin.tie();

	cin >> n >> m;

	for (int i = 0; i < m; i++){ //간선마다 노드, 비용값을 넣는다 
		cin >> A >> B >> C;
		adj[A].push_back({ B,C });}
	
	for (int i = 1; i <= n; i++) ans[i] = INF; // 모든 노드를 INF로 세팅

	ans[1] = 0; // 시작점 0으로 초기화
	
	for (int y = 1; y <= n; y++)
		for (int x = 1; x <= n; x++){
			for (auto next: adj[x]){
				int nn = next.first; int nc = next.second;
				
				if(ans[x] == INF) continue; //이건 왜할까? 

                		if(ans[nn]>ans[x]+nc){
                   			ans[nn]= ans[x]+nc; // update된 경우에
                    			if (y == n) cycle_of_minus = true;}
		}}
	
	if (cycle_of_minus) cout << -1 << '\n';
	else{
		for (int i = 2; i <= n; i++)
			cout << (ans[i] != INF ? ans[i] : -1) << '\n';}}
