#include<iostream>
#include<vector>

using namespace std;
struct NUM{
    int a,b;
};

int main(){
    vector<NUM> res;
    int n,fir,sec;
    cin>>n;
    char c;
    for(int i=0;i<n;++i){
        cin>>fir;
        cin>>c;
        cin>>sec;
        res.push_back({fir,sec});
    }
    
    for(int i=0; i<res.size() ;++i){
        cout<<res[i].a + res[i].b <<endl;
    }
    
return 0;   
}


