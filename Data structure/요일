//백준 1924

#include<iostream>
#include<string>

using namespace std;


int main(){
    int month,day;
    cin>>month;
    cout<<" ";
    cin>>day;
    solve();
return 0;   }

void solve(){
    
    int date= (month-1)*30+day;
    if(month>2){date-=3;}
    if(month>3){date+=1;}
    if(month>5){date+=1;}
    if(month>7){date+=1;}
    if(month>8){date+=1;}
    if(month>10){date+=1;}
      
    if(day/7==0) cout<<"SUN";
    if(day/7==1) cout<<"SUN";
    if(day/7==2) cout<<"SUN";
    if(day/7==3) cout<<"SUN";
    if(day/7==4) cout<<"SUN";
    if(day/7==5) cout<<"SUN";
    if(day/7==5) cout<<"SUN";
    
}
