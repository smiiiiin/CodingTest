//백준 1212 //이건 왜 안될까
#include<iostream>
#include<vector>
#include<string>
#include<algorithm>

using namespace std;
vector <int> v;

int main(){
    cin.tie(0); cout.tie(0); ios_base::sync_with_stdio(false);
    
    string s="";
    getline(cin,s);
    
    for(int i=s.size()-1;i>=0;i--){
        s[i]-='0';
        while(s[i]>0){
        int tmp=s[i] %2;
            s[i]/=2;
        v.push_back(tmp);
        }
        while(v.size()!=3)
        {v.push_back(0);}
    }
    reverse(v.begin(),v.end());
    for(auto i:v) cout<<i;
    cout<<"\n";
    return 0;
}
